{
    "name": "harmony/routing-bundle",
    "type": "symfony-bundle",
    "description": "Harmony RoutingBundle",
    "keywords": ["routing", "database", "orm", "mongodb"],
    "homepage": "",
    "license": "MIT",
    "authors": [
        {
            "name": "Symfony CMF Community",
            "homepage": "https://github.com/symfony-cmf/RoutingBundle/contributors"
        },
        {
            "name": "David Sanchez",
            "email": "david38sanchez@gmail.com",
            "homepage": "http://davidsanchez.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "symfony-cmf/routing": "^2.1.0",
        "symfony/framework-bundle": "^4.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.2.2",
        "matthiasnoback/symfony-dependency-injection-test": "^2.3.1",
        "matthiasnoback/symfony-config-test": "^3.1.1",
        "symfony-cmf/testing": "^2.1.11",
        "doctrine/data-fixtures": "^1.0.0",
        "symfony/validator": "^2.8 || ^3.3 || ^4.0"
    },
    "suggest": {
        "emulienfou/orm-pack": "A Symfony Pack for Doctrine ORM",
        "emulienfou/mongodb-pack": "A pack for the Doctrine MongoDB ODM"
    },
    "conflict": {
        "twig/twig": "<2",
        "phpunit/phpunit": "<6"
    },
    "autoload": {
        "psr-4": {
            "Harmony\\Bundle\\RoutingBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Harmony\\Bundle\\RoutingBundle\\Tests\\": "tests/"
        }
    }
}
