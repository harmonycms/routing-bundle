<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="harmony_routing.initializer" class="Doctrine\Bundle\PHPCRBundle\Initializer\GenericInitializer">
            <argument>HarmonyRoutingBundle</argument>
            <argument>%harmony_routing.dynamic.persistence.phpcr.initialized_basepaths%</argument>
            <tag name="doctrine_phpcr.initializer"/>
        </service>

    </services>
</container>
